{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.minimap.bound_to_edges": {
      "geometry": "Geometry.minimap",
      "materials": [{ "*": "Material.animated" }],
      "textures": [ "Texture.minimap" ],
      "is_hurt_color": {},
      "on_fire_color": {},
      "uv_anim": {
        "offset": [ "math.clamp((q.position(0) + (-v.cor_x - (v.zoom_x * v.res_x / 2))) / v.res_x, 0, v.max_offset_x)", "math.clamp((q.position(2) + (-v.cor_z - (v.zoom_z * v.res_z / 2))) / v.res_z, 0, v.max_offset_z)" ], 
        "scale": [ "v.zoom_x", "v.zoom_z" ]
      },
      "part_visibility": [{ "bg": false }]
    },
    "controller.render.minimap.not_bound_to_edges": {
      "geometry": "Geometry.minimap",
      "materials": [{ "*": "Material.animated" }],
      "textures": [ "Texture.minimap" ],
      "is_hurt_color": {},
