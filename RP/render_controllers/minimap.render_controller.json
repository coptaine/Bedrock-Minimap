      "uv_anim": {
        "offset": [ "(q.position(0) + (-v.cor_x - (v.zoom_x * v.res_x / 2))) / v.res_x", "(q.position(2) + (-v.cor_z - (v.zoom_z * v.res_z / 2))) / v.res_z" ], 
        "scale": [ "v.zoom_x", "v.zoom_z" ]
      },
      "part_visibility": [{ "bg": false }]
    },
    "controller.render.minimap_bg": {
      "geometry": "Geometry.minimap",
